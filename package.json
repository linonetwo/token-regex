{
  "name": "token-regex-it",
  "version": "1.0.0",
  "description": "Data extraction on tokens of segmented sentences. In any JS environment (browser, election, IOT...)",
  "author": "linonetwo",
  "license": "Apache-2.0",
  "homepage": "https://github.com/linonetwo/token-regex",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack -p --progress --mode development --watch --config webpack.config.dev.js",
    "generate-parser": "antlr4 -o ./src/antlrGeneratedParser -visitor -Dlanguage=JavaScript ./TokenRegexIt.g4",
    "view-tree": "node ./scripts/viewParseTree.js",
    "build": "webpack -p --progress --mode production --config webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:linonetwo/token-regex.git"
  },
  "dependencies": {
    "antlr4": "4.7.1",
    "core-decorators": "0.20.0",
    "lodash": "4.17.10",
    "param.macro": "2.1.0",
    "preval.macro": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-decorators": "7.2.0",
    "@babel/plugin-proposal-do-expressions": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/runtime": "7.2.0",
    "babel-eslint": "8.2.6",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-dynamic-import-node-babel-7": "2.0.7",
    "babel-plugin-flow-runtime": "0.17.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-macros": "2.4.0",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "eslint": "5.4.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.22.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.79.1",
    "flow-runtime": "0.17.0",
    "flow-typed": "2.5.1",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-merge": "4.1.4"
  }
}
